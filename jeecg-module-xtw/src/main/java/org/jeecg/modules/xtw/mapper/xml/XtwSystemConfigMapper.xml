<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.xtw.mapper.XtwSystemConfigMapper">

    <select id="findConfig" parameterType="String" resultType="org.jeecg.modules.xtw.entity.XtwSystemConfig">
        SELECT * FROM xtw_system_config
        WHERE config_key = #{configKey}
    </select>

</mapper>